{
  "$schema": "./node_modules/@angular-devkit/schematics/collection-schema.json",
  "schematics": {
    "auth": {
      "description": "Generate a auth lib module.",
      "factory": "./dist/auth/auth.factory#main"
    },
    "auth-app": {
      "description": "Generate a auth app module.",
      "factory": "./dist/auth-app/auth-app.factory#main"
    },
    "ddd-model": {
      "description": "Generate a model.",
      "factory": "./dist/ddd/model/model.factory#main"
    },
    "ddd-agg-root": {
      "description": "Generate an aggregate.",
      "factory": "./dist/ddd/aggregate-root/aggregate-root.factory#main"
    },
    "ddd-entity": {
      "description": "Generate an entity.",
      "factory": "./dist/ddd/entity/entity.factory#main"
    },
    "ddd-usecases": {
      "description": "Generate a usecase collection.",
      "factory": "./dist/ddd/usecases/usecases.factory#main"
    },
    "ddd-error": {
      "description": "Generate a domain error.",
      "factory": "./dist/ddd/error/error.factory#main"
    }
  }
}
