    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.27.5",
    "jasmine": "^4.5.0",
    "jest": "27.5.1",
    "pluralize": "^8.0.0",
    "prettier": "2.8.3",
    "socket.io-client": "^4.7.2",
