{
  "name": "simplefeed-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "PROFILE=local nx serve simplefeed",
    "predev": "PROFILE=local npm run schema:create",
    "build": "nx build",
    "test:libs": "PROFILE=test NODE_ENV=test nx run-many --projects=post,user,auth,chat --target=test --parallel=false",
    "test:app": "PROFILE=test NODE_ENV=test nx test simplefeed --runInBand --skip-nx-cache",
    "infra:up": "docker compose up -d",
    "infra:down": "docker compose down",
    "schema:create": "ts-node --project tsconfig.script.json apps/simplefeed/scripts/create-schema",
    "schema:create:prod": "ts-node --project tsconfig.script.json apps/simplefeed/scripts/create-schema",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "npm run lint -- --fix",
    "start:prod": "node dist/apps/simplefeed/main.js",
    "prestart:prod": "node dist/apps/scripts/main.js"
  },
  "schematics": "./libs/schematics/collection.json",
  "private": true,
  "dependencies": {
    "@casl/ability": "^6.0.0",
    "@elastic/elasticsearch": "^7.17.11",
    "@nestjs/common": "^10.0.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.3",
    "@nestjs/cqrs": "^10.0.1",
    "@nestjs/elasticsearch": "^10.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.3",
    "@nestjs/platform-socket.io": "^10.0.3",
    "@nestjs/platform-ws": "^10.2.9",
    "@nestjs/schedule": "^3.0.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.0.3",
    "@nrwl/node": "14.5.8",
    "@socket.io/redis-adapter": "^8.2.1",
    "@socket.io/redis-emitter": "^5.1.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.18",
    "@types/multer": "^1.4.8",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/socket.io": "^3.0.2",
    "aws-sdk": "^2.1395.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "joi": "^17.6.0",
    "nestjs-typeorm-paginate": "^4.0.4",
    "nx": "14.5.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "short-uuid": "^4.2.0",
    "slugify": "^1.6.5",
    "tslib": "^2.3.0",
    "typeorm": "^0.3.7",
    "typeorm-transactional": "^0.2.1",
    "types-joi": "^2.1.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.1",
