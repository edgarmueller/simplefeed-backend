{
  "modules": {
    "user": {
      "type": "domain",
      "User": {
        "type": "AggregateRoot",
        "props": {
          "email": {
            "type": "string",
            "queryable": true,
            "unique": true
          },
          "password": {
            "type": "string"
          },
          "profile": {
            "type": "Profile",
            "multiplicity": "one-to-one",
            "inverseSide": "user",
            "meta": {
              "orm": {
                "eager": true,
                "cascade": true
              }
            }
          },
          "createdAt": { 
            "type": "Date" ,
            "meta": {
              "orm": {
                "createdAt": true
              }
            }
          },
          "updatedAt": { 
            "type": "Date", 
            "meta": {
              "orm": {
                "updatedAt": true
              }
            }
          }
        }
      },
      "Profile": {
        "type": "Entity",
        "props": {
          "username": {
            "type": "string",
            "queryable": true
          },
          "bio": { 
            "type": "string", 
            "optional": true
          },
          "firstName": { 
            "type": "string"
          },
          "lastName": { 
            "type": "string" 
          },
          "imageUrl": { 
            "type": "string",
            "optional": true
          },  
          "nrOfPosts": {
            "type": "number",
            "optional": true,
            "default": 0
          },
          "nrOfLikes": {
            "type": "number",
            "optional": true,
            "default": 0
          },
          "follows": {
            "type": "Profile",
            "multiplicity": "many-to-many",
            "inverseSide": "followedBy",
            "optional": true
          },
          "user": { 
            "type": "User",
            "multiplicity": "one-to-one",
            "inverseSide": "profile",
            "optional": true,
            "onDelete": "CASCADE"
          },
          "createdAt": { 
            "type": "Date",
            "metadata": "createdAt"
          },
          "updatedAt": { 
            "type": "Date",
            "metadata": "updatedAt"
          }
        }
      }
    },
    "post": {
      "type": "domain",
      "Post": {
        "type": "AggregateRoot",
        "props": {
          "addedBy": {
            "type": "string"
          },
          "postedTo": {
            "type": "string"
          },
          "createdAt": {
            "type": "Date",
            "meta": {
              "orm": {
                "createdAt": true
              }
            }
          },
          "userClosed": {
            "type": "boolean",
            "default": false
          },
          "likes": {
            "type": "number"
          },
          "user": {
            "type": "User",
            "multiplicity": "many-to-one"
          }
        }
      },
      "Comment": {
        "type": "Entity",
        "props": {
          "postedBy": {
            "type": "string"
          },
          "postedTo": {
            "type": "string"
          },
          "removed": {
            "type": "boolean",
            "default": false
          },
          "createdAt": {
            "type": "Date",
            "meta": {
              "orm": {
                "createdAt": true
              }
            }
          },
          "post": {
            "type": "Post",
            "multiplicity": "many-to-one"
          }
        }
      },
      "Like": {
        "username": {
          "type": "string"
        },
        "post": {
          "type": "Post",
          "multiplicity": "many-to-one"
        }
      }
    },
    "auth": {
      "type": "auth",
      "refreshToken": {
        "enabled": true
      }
    }
  },
  "routes": {}
}